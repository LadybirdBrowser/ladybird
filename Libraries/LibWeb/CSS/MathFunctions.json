      {
        "name": "value",
        "type": "<number>|<dimension>|<percentage>",
        "required": true
      }
    ]
  },
  "acos": {
    "parameters": [
      {
        "name": "value",
        "type": "<number>",
        "required": true
      }
    ]
  },
  "asin": {
    "parameters": [
      {
        "name": "value",
        "type": "<number>",
        "required": true
      }
    ]
  },
  "atan": {
    "parameters": [
