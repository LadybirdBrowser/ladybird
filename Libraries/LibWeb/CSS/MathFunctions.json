  "sqrt": {
    "parameters": [
      {
        "name": "value",
        "type": "<number>",
        "required": true
      }
    ]
  },
  "tan": {
    "parameters": [
      {
        "name": "value",
        "type": "<number>|<angle>",
        "required": true
      }
    ]
  }
