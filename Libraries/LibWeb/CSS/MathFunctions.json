        "required": true
      }
    ]
  },
  "exp": {
    "parameters": [
      {
        "name": "value",
        "type": "<number>",
        "required": true
      }
    ]
  },
  "hypot": {
    "parameter-validation": "consistent",
    "is-variadic": true,
    "parameters": [
      {
        "name": "value",
        "type": "<number>|<dimension>|<percentage>",
        "required": true
      }
    ]
  },
  "log": {
    "parameter-validation": "same",
    "parameters": [
      {
        "name": "value",
        "type": "<number>",
        "required": true
      },
      {
        "name": "base",
        "type": "<number>",
        "required": false,
        "default": "e"
      }
    ]
  },
  "max": {
    "parameter-validation": "consistent",
